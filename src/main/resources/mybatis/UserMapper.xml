<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.trons.mapper.UserMapper">
    <select id="findUserInfo" resultType="com.trons.model.User">
        select name, age,password from user where id = #{id};
    </select>

    <select id="getAllUser" resultType="com.trons.model.User">
        select name, age,password from user;
    </select>
</mapper>